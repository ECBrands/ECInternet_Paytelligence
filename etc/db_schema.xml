<?xml version="1.0"?>
<schema xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:framework:Setup/Declaration/Schema/etc/schema.xsd">
  <table name="ecinternet_paytelligence_card" resource="default">
    <column xsi:type="int" name="entity_id" padding="10" unsigned="true" nullable="false" identity="true" comment="Entity Id" />
    <column xsi:type="timestamp" name="created_at" on_update="false" nullable="false" default="CURRENT_TIMESTAMP" comment="Created At" />
    <column xsi:type="timestamp" name="updated_at" on_update="true" nullable="false" default="CURRENT_TIMESTAMP" comment="Updated At" />
    <column xsi:type="int" name="CARDID" padding="10" unsigned="true" nullable="false" comment="Card Id" />
    <column xsi:type="int" name="AUDTDATE" padding="10" nullable="false" comment="Audit Date" />
    <column xsi:type="int" name="AUDTTIME" padding="11" nullable="false" comment="Audit Time" />
    <column xsi:type="varchar" name="AUDTUSER" nullable="false" length="8" comment="Audit User" />
    <column xsi:type="varchar" name="AUDTORG" nullable="false" length="6" comment="Audit Organization" />
    <column xsi:type="varchar" name="MERCODE" nullable="false" length="12" comment="Merchant Code" />
    <column xsi:type="varchar" name="SHIPTO" nullable="false" length="6" comment="Ship-To" />
    <column xsi:type="smallint" name="CARDSTTE" padding="6" unsigned="true" nullable="false" comment="Card State" />
    <column xsi:type="varchar" name="PROFILID" nullable="false" length="64" comment="Profile Id" />
    <column xsi:type="varchar" name="CARDMASK" nullable="false" length="20" comment="Card Mask" />
    <column xsi:type="varchar" name="CUSTOMER" nullable="false" length="12" comment="Customer Number" />
    <column xsi:type="smallint" name="CARDTYPE" padding="6" unsigned="true" nullable="false" comment="Card Type" />
    <column xsi:type="varchar" name="CARDNAME" nullable="false" length="60" comment="Card Name" />
    <column xsi:type="varchar" name="DRIVLIC" nullable="false" length="60" comment="Driver's License" />
    <column xsi:type="varchar" name="IPADDR" nullable="false" length="15" comment="IP Address" />
    <column xsi:type="varchar" name="BILADDR1" nullable="false" length="60" comment="Bill-To Street Line 1" />
    <column xsi:type="varchar" name="BILADDR2" nullable="false" length="60" comment="Bill-To Street Line 2" />
    <column xsi:type="varchar" name="BILCITY" nullable="false" length="30" comment="Bill-To City" />
    <column xsi:type="varchar" name="BILPROV" nullable="false" length="30" comment="Bill-To State/Province" />
    <column xsi:type="varchar" name="BILCTRY" nullable="false" length="30" comment="Bill-To Country" />
    <column xsi:type="varchar" name="BILPSTL" nullable="false" length="20" comment="Bill-To Postal Code" />
    <column xsi:type="varchar" name="CNTPHONE" nullable="false" length="30" comment="Phone" />
    <column xsi:type="smallint" name="EXPYEAR" unsigned="true" nullable="false" comment="Expiration Year" />
    <column xsi:type="smallint" name="EXPMONTH" unsigned="true" nullable="false" comment="Expiration Month" />
    <column xsi:type="varchar" name="COMMENT" nullable="false" length="255" comment="Comment" />
    <column xsi:type="decimal" name="TRNSLMT" scale="6" precision="19" unsigned="true" nullable="false" comment="Transaction Limit" default="0" />
    <column xsi:type="varchar" name="CARDPWD" nullable="false" length="20" comment="Card Password" />
    <column xsi:type="boolean" name="DFLTCARD" nullable="false" comment="Is Default Card?" default="false" />
    <column xsi:type="varchar" name="AVS" nullable="false" length="60" comment="AVS" />
    <column xsi:type="varchar" name="CVV" nullable="false" length="60" comment="CVV" />
    <column xsi:type="boolean" name="ISSTORED" nullable="false" comment="Is Card Stored?"  />
    <column xsi:type="varchar" name="EMAIL" nullable="false" length="100" comment="Email" />
    <column xsi:type="boolean" name="SWIPEOPT" nullable="false" comment="Was Card Swiped?" default="false" />
    <column xsi:type="varchar" name="APPROFILEID" nullable="false" length="64" comment="APPROFILEID" />
    <column xsi:type="text" name="CCNETWORK" nullable="false" comment="Credit Card Network" />
    <column xsi:type="int" name="last_order_id" unsigned="true" nullable="false" comment="Last Order Id" />
    <constraint xsi:type="primary" referenceId="PRIMARY">
      <column name="entity_id" />
    </constraint>
    <index referenceId="ECINTERNET_PAYTELLIGENCE_CARD_CARDID" indexType="btree">
      <column name="CARDID" />
    </index>
    <index referenceId="ECINTERNET_PAYTELLIGENCE_CARD_PROFILID" indexType="btree">
      <column name="PROFILID" />
    </index>
  </table>
  <table name="ecinternet_paytelligence_trans" resource="default">
    <column xsi:type="int" name="entity_id" padding="10" unsigned="true" nullable="false" identity="true" comment="Entity Id" />
    <column xsi:type="int" name="parent_card_id" padding="11" unsigned="true" nullable="false" comment="Parent Card Id" />
    <column xsi:type="timestamp" name="created_at" on_update="false" nullable="false" default="CURRENT_TIMESTAMP" comment="Created At" />
    <column xsi:type="timestamp" name="updated_at" on_update="true" nullable="false" default="CURRENT_TIMESTAMP" comment="Updated At" />
    <column xsi:type="decimal" name="TRNSNUM" precision="10" scale="0" unsigned="true" nullable="false" comment="Transaction Number" />
    <column xsi:type="decimal" name="CARDID" precision="10" scale="0" unsigned="true" nullable="false" comment="Card Id" />
    <column xsi:type="varchar" name="CARDNUM" nullable="false" length="20" comment="Card Number" />
    <column xsi:type="varchar" name="MERCODE" nullable="false" length="12" comment="Merchant Code" />
    <column xsi:type="varchar" name="PARENTID" nullable="false" length="12" comment="Parent Id" />
    <column xsi:type="int" name="TRNSSRC" padding="10" unsigned="true" nullable="false" comment="Transaction Source" default="9" />
    <column xsi:type="int" name="AUTHED" padding="10" unsigned="true" nullable="false" comment="Authorized" />
    <column xsi:type="int" name="TRNSTYPE" padding="10" unsigned="true" nullable="false" comment="Transaction Type" />
    <column xsi:type="int" name="TRNSSTTE" padding="10" unsigned="true" nullable="false" comment="Transaction State" />
    <column xsi:type="decimal" name="TRNSDATE" precision="10" scale="0" unsigned="true" nullable="false" comment="Transaction Date" />
    <column xsi:type="decimal" name="TRNSTIME" precision="10" scale="0" unsigned="true" nullable="false" comment="Transaction Time" />
    <column xsi:type="varchar" name="DOCCURR" nullable="false" length="22" comment="Document Currency" />
    <column xsi:type="decimal" name="DOCAMT" precision="19" scale="3" unsigned="true" nullable="false" comment="Document Amount" />
    <column xsi:type="varchar" name="TRNSCURR" nullable="false" length="3" comment="Transaction Currency" />
    <column xsi:type="decimal" name="TRNSAMT" precision="19" scale="3" unsigned="true" nullable="false" comment="Transaction Amount" />
    <column xsi:type="decimal" name="CONV" precision="19" scale="3" unsigned="true" nullable="false" comment="Conversion" default="1" />
    <column xsi:type="int" name="RATEOP" padding="10" unsigned="true" nullable="false" comment="Rate Operator" default="1" />
    <column xsi:type="decimal" name="AUTHAMT" precision="19" scale="3" unsigned="true" nullable="false" comment="Authorization Amount" />
    <column xsi:type="decimal" name="CAPAMT" precision="19" scale="3" unsigned="true" nullable="false" comment="Capture Amount" />
    <!-- REFAMT ? -->
    <column xsi:type="decimal" name="ADDCHRG" precision="19" scale="3" unsigned="true" nullable="false" comment="Additional Charge" default="0" />
    <column xsi:type="varchar" name="ORDERNUM" nullable="false" length="22" comment="Order Number" />
    <column xsi:type="varchar" name="PONUM" nullable="false" length="22" comment="Purchase Order Number" />
    <column xsi:type="varchar" name="DOCNUM" nullable="false" length="22" comment="Document Number" />
    <column xsi:type="int" name="DOCTYPE" padding="10" unsigned="true" nullable="false" comment="Document Type" default="0" />
    <column xsi:type="varchar" name="CUSTOMER" nullable="false" length="12" comment="Customer Number" />
    <column xsi:type="varchar" name="LOCATION" nullable="false" length="6" comment="Location Code" />
    <column xsi:type="varchar" name="GWTRNSID" nullable="false" length="20" />
    <column xsi:type="varchar" name="GWREFCDE" nullable="false" length="20" />
    <column xsi:type="varchar" name="GWORDRID" nullable="false" length="20" />
    <column xsi:type="varchar" name="TRNSCMT" nullable="false" length="150" />
    <column xsi:type="varchar" name="TRNSRESD" nullable="false" length="60" />
    <column xsi:type="varchar" name="GLACCTNO" nullable="false" length="45" />
    <column xsi:type="int" name="ARBNUM" padding="10" unsigned="true" nullable="false" default="0" />
    <column xsi:type="int" name="ARBENUM" padding="10" unsigned="true" nullable="false" default="0" />
    <column xsi:type="int" name="ASDCTYPE" padding="10" unsigned="true" nullable="false" default="0" />
    <column xsi:type="varchar" name="ASDCNUM" nullable="false" length="22" />
    <column xsi:type="varchar" name="CNTPAYM" nullable="true" length="12" />
    <column xsi:type="int" name="TESTMODE" padding="11" unsigned="true" nullable="false" />
    <column xsi:type="varchar" name="increment_id" nullable="false" length="32" comment="Magento Order Increment ID" />
    <column xsi:type="int" name="order_id" unsigned="true" nullable="false" comment="Magento Order Id" />
    <column xsi:type="int" name="customer_id" padding="11" unsigned="true" nullable="false" comment="Magento Customer ID" />
    <column xsi:type="varchar" name="area_code" nullable="false" length="20" comment="Area Code" />
    <column xsi:type="varchar" name="sage_idcust" nullable="false" length="12" comment="Sage Customer Number" />
    <column xsi:type="int" name="sage_nntrans_id" padding="11" unsigned="true" nullable="false" default="0" comment="Sage NNTRANS ID" />
    <constraint xsi:type="primary" referenceId="PRIMARY">
      <column name="entity_id" />
    </constraint>
  </table>
  <table name="ecinternet_paytelligence_log" resource="default">
    <column xsi:type="int" name="entity_id" padding="10" unsigned="true" nullable="false" identity="true" comment="Entity Id" />
    <column xsi:type="timestamp" name="created_at" on_update="false" nullable="false" default="CURRENT_TIMESTAMP" comment="Created At" />
    <column xsi:type="timestamp" name="updated_at" on_update="true" nullable="false" default="CURRENT_TIMESTAMP" comment="Updated At" />
    <column xsi:type="varchar" name="order_number" nullable="false" length="32" comment="Order Number" />
    <column xsi:type="varchar" name="payment_gateway" nullable="false" length="50" comment="Payment Gateway" />
    <column xsi:type="varchar" name="area_code" nullable="false" length="20" comment="Area Code" />
    <column xsi:type="varchar" name="log_type" nullable="false" length="50" comment="Log Type" />
    <column xsi:type="text" name="value" nullable="false" comment="Value" />
    <constraint xsi:type="primary" referenceId="PRIMARY">
      <column name="entity_id" />
    </constraint>
  </table>
</schema>
