		</settings>
		<dataProvider class="Magento\Framework\View\Element\UiComponent\DataProvider\DataProvider" name="paytelligence_card_listing_data_source">
			<settings>
				<requestFieldName>entity_id</requestFieldName>
				<primaryFieldName>entity_id</primaryFieldName>
			</settings>
		</dataProvider>
	</dataSource>

